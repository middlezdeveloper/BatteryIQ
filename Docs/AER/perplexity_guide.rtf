\pard\tx940\tx1440\pardeftab720\li1440\fi-1440\sa120\partightenfactor0
\ls4\ilvl1
\f0 \AppleTypeServices\AppleTypeServicesF65539 \cf2 \cb3 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 {\listtext	\uc0\u8226 	}\expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec2 Unrestricted accreditation
\f1 \AppleTypeServices\AppleTypeServicesF65539 \'a0(full CDR recipient; most compliance obligations, insurance, security, dispute resolution, etc.),\cb1 \
\ls4\ilvl1
\f0 \AppleTypeServices\AppleTypeServicesF65539 \cb3 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 {\listtext	\uc0\u8226 	}\expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec2 Sponsored accreditation
\f1 \AppleTypeServices\AppleTypeServicesF65539 \'a0(lighter evidence/app but only via a sponsor, restrictions apply),\cb1 \
\ls4\ilvl1
\f0 \AppleTypeServices\AppleTypeServicesF65539 \cb3 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 {\listtext	\uc0\u8226 	}\expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec2 CDR Representative/OSP models
\f1 \AppleTypeServices\AppleTypeServicesF65539 \'a0allow you to build on another accredited provider\'92s CDR environment (not typically needed for pricing comparison sites).\cb1 \
